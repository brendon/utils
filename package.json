{
  "name": "@daybrush/utils",
  "version": "0.5.1",
  "description": "utils for daybrush",
  "main": "dist/utils.js",
  "module": "dist/utils.esm.js",
  "sideEffects": false,
  "types": "declaration/index.d.ts",
  "keywords": [
    "daybrush",
    "utils",
    "dom",
    "css",
    "color",
    "rgba",
    "hsla",
    "splitSpace",
    "splitQuoto",
    "splitComma",
    "splitBracket",
    "camelize",
    "decamelize",
    "now",
    "requestAnimationFrame",
    "transform",
    "filter",
    "animation",
    "crossbrowser",
    "rgb",
    "hsl",
    "isUndefined",
    "isFunction",
    "isString",
    "isNumber"
  ],
  "scripts": {
    "build": "npm run build:rollup && npm run declaration",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
    "build:rollup": "rollup -c",
    "doc": "rm -rf ./doc && jsdoc -c jsdoc.json",
    "deploy": "gh-pages -d ./demo --dest=./ --add",
    "release": "npm run build && npm run release:current && npm run release:latest && npm run release:doc  && npm run deploy",
    "release:mkdir": "mkdir -p ./demo/release",
    "release:current": "npm run release:mkdir && rm -rf ./demo/release/$npm_package_version && cp -a ./dist/. ./demo/release/$npm_package_version",
    "release:latest": "npm run release:mkdir && rm -rf ./demo/release/latest && cp -a ./dist/. ./demo/release/latest",
    "release:doc": "npm run doc && cp -a ./doc/. ./demo/release/latest/doc && cp -a ./doc/. ./demo/release/$npm_package_version/doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daybrush/utils"
  },
  "author": "Daybrush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daybrush/utils/issues"
  },
  "homepage": "https://github.com/daybrush/utils#readme",
  "devDependencies": {
    "@daybrush/jsdoc": "^0.3.1",
    "daybrush-jsdoc-template": "^1.5.0",
    "gh-pages": "^2.0.1",
    "rollup": "^0.66.6",
    "rollup-plugin-es3": "^1.1.0",
    "rollup-plugin-prototype-minify": "^1.0.4",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "string-replace": "^0.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {}
}
